#include <iostream>
#include <vector>
using namespace std;
int main() {
	vector <int> v1, v2, v3;
	vector <int>::iterator iter;
	v1.push_back(10); v2.push_back(1); v1.push_back(20); v2.push_back(2);
	v1.push_back(30); v2.push_back(3); v1.push_back(40); v2.push_back(4);
	v1.push_back(50); v2.push_back(5);
	cout << "v1[2] = " << v1[2] << endl;
	cout << " The size of v1 is " << v1.size() << endl;
	cout << " v1 = ";
	for (iter = v1.begin(); iter != v1.end(); iter++) {
		cout << " " << *iter;
	}
	cout << endl;
	v1.push_back(60);
	v1.erase(v1.begin() + 1);
	cout << " The size of v1 is " << v1.size() << endl;
	cout << " v1 = ";
	for (iter = v1.begin(); iter != v1.end(); iter++) {
		cout << " " << *iter;
	}
	cout << " after erase\n";
	v1.clear();
	cout << " The size of v1 after clearing is " << v1.size() << endl;
	return 123;
}
