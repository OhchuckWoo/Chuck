void setup(){
  student siha, nouk;  // 2 objects for class student
  siha = new student(); // set default values
  nouk = new student(); // set default values
  println(siha.h + " " + siha.w); // print member data
  siha.phen();         // call the member function
  println(siha.r);
  nouk.w = 90;         // change value of a member data
  nouk.phen();  
  println(nouk.h + " " + nouk.w);
  println(nouk.r);
  println(siha.grade);
  println(nouk.grade);
  siha.judge();
  nouk.judge();
}

// a class with 3 member data and 1 member function 
class student {     // class name is student
    // constructor
    student( ) { w = 60; h = 180; grade = int(random(60, 100));}
    // member data
    float w, h, r;
    int grade;
    // member function
    void phen( ){   
         r = h/w;
    }
    void judge( ){
      if (grade > 80) println("Good!");
      else println("Study!");
    }
}
